{
  "name": "sequence-diagram",
  "version": "1.0.0",
  "description": "Generates SVG Sequence Diagrams from code in the browser",
  "homepage": "https://github.com/davidje13/SequenceDiagram",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidje13/SequenceDiagram.git"
  },
  "bugs": {
    "url": "https://github.com/davidje13/SequenceDiagram/issues"
  },
  "license": "LGPL-3.0",
  "files": [
    "lib/sequence-diagram.js"
  ],
  "main": "lib/sequence-diagram",
  "module": "source/standalone",
  "scripts": {
    "lint": "eslint scripts --ext .js --ignore-pattern '!.eslintrc.js' --ignore-pattern '*FontData.js'",
    "minify": "rollup --config rollup.config.js && uglifyjs --compress --mangle --warn --output lib/sequence-diagram.min.js -- lib/sequence-diagram.js && uglifyjs --compress --mangle --warn --output weblib/editor.min.js -- weblib/editor.js",
    "start": "http-server"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-jasmine": "^2.9.3",
    "http-server": "^0.10.0",
    "requirejs": "2.3.5",
    "rollup": "^0.57.1",
    "rollup-plugin-hypothetical": "^2.1.0",
    "uglify-es": "^3.1.10"
  }
}
